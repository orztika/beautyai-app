{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app.py": {
      "excludeFiles": [
        ".git/**",
        ".gitignore",
        "README.md",
        "DEPLOYMENT_GUIDE.md",
        "main.py",
        ".env.example",
        "runtime.txt"
      ]
    }
  }
}